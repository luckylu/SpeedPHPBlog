body,html{background-image:url(../img/wood.png);
width:800px;

}
.main{ 
width:800px;
margin:100px 0 0 240px;

}
ul.cat{
margin:0 auto;
padding:0 auto;
list-style:none;


}
ul.cat li{
float:left;
margin:0 0 0 10px;
padding:0 auto;
overflow:hidden;

}
.catmain{
margin:0 auto;
float:left;

}
.btn{
clear:both;
float:left;
margin:10px auto;
padding:0 auto;

}
.customers
  {
  font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
  width:100%;
  border-collapse:collapse;
  }

.customers td, .customers th 
  {
  font-size:1em;
  border:1px solid #98bf21;
  padding:3px 7px 2px 7px;
  }

.customers th 
  {
  font-size:1.1em;
  text-align:left;
  padding-top:5px;
  padding-bottom:4px;
  background-color:#A7C942;
  color:#ffffff;
  }
.even{
  color:#000000;
  background-color:#EAF2D3;
}
.odd{
  color:#000000;
  background-color:#fdeeed;
}